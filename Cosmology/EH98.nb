      RowBox[{"ag", " ", "=", " ", 
       RowBox[{"1.0", " ", "-", " ", 
        RowBox[{"0.328", "*", 
         RowBox[{"Log", "[", 
          RowBox[{"431", "*", "om"}], "]"}], " ", "*", " ", 
         RowBox[{"(", 
          RowBox[{"ob", "/", "om"}], ")"}]}], " ", "+", " ", 
        RowBox[{"0.38", " ", "*", " ", 
         RowBox[{"Log", "[", 
          RowBox[{"22.3", "*", "om"}], "]"}], "*", 
